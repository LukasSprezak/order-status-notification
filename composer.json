{
    "name": "order-status/order-status-notification",
    "description": "Provides notification of order status changes",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "OrderStatusNotification\\": "src/"
        }
    },
    "authors": [
        {
            "name": "lsprezak",
            "email": "lsprezak@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.3",
        "ext-soap": "*",
        "symfony/validator": "6.4.*",
        "guzzlehttp/guzzle": "7.9.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload-dev": {
        "psr-4": {
            "OrderStatusNotification\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
